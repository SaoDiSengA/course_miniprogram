button::after{ 
	border: none; 
}

button {
  position: fixed;
    top: 85%;
    left: 5%;
    line-height: 100rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 30px;
    background: #698EF7;
    color: white; 
    padding: 0rpx 100rpx;
}