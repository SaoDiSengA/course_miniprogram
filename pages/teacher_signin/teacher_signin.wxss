button::after{ 
	border: none; 
}

button {
  line-height: 100rpx;
  position:fixed;
  left: 5%;
  top: 85%;
  border-radius: 30px;
  background: #698EF7;
  color: white; 
  padding: 0rpx 100rpx;
}