/* pages/system_notion/system_notion.wxss */

/* 整体页面样式 */
.container {
    padding: 0px;
}

/* 消息详情列表 */
.notion_infos {
    width: 90%;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 10px;
    background-color: #ffffff;
}

/* 单个消息显示 */
.notion_info {
    display: flex;
    flex-direction: row;
    justify-content: left;
}


